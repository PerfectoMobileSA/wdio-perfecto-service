{
  "name": "wdio-perfectotest-service",
  "version": "1.0.12",
  "description": "WebdriverIO service for Perfecto Lab integration",
  "keywords": [
    "perfecto",
    "webdriverio",
    "wdio",
    
    "wdio-service"
  ],
  "contributors": [
    "Hadas Shani <hshani@perforce.com>"
  ],
  "homepage": "https://github.com/PerfectoMobileSA/wdio-perfecto-service",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PerfectoMobileSA/wdio-perfecto-service.git"
  },
  "bugs": {
    "url": "https://github.com/PerfectoMobileSA/wdio-perfecto-service/issues"
  },
  "main": "./build/index.js",
  "types": "./perfecto-service.d.ts",
  "files": [
    "build/",
    "perfecto-service.d.ts"
  ],
  "engines": {
    "node": ">= 12.0.0"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf build node_modules",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint -c .eslintrc.js --ext .ts .",
    "lintfix": "eslint -c .eslintrc.js --fix --ext .ts .",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run build",
    "preversion": "npm run lint",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@wdio/logger": "^7.4.2",
    "cucumber-tag-expressions": "^2.0.3",
    "integer": "^2.1.0",
    "lodash": "^4.17.21",
    "wdio": "^4.0.0"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.7",
    "@tsconfig/node14": "^1.0.0",
    "@types/glob": "^7.1.3",
    "@types/integer": "^1.0.1",
    "@types/node": "^14.17.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.13.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@wdio/cli": "^6.10.10",
    "@wdio/cucumber-framework": "^6.11.1",
    "@wdio/local-runner": "^6.10.10",
    "@wdio/mocha-framework": "^6.10.10",
    "@wdio/spec-reporter": "^6.10.10",
    "@wdio/sync": "^6.10.10",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^7.1.0",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "perfecto-reporting": "^2.5.3",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,ts,md}": "prettier --write"
  }
}
